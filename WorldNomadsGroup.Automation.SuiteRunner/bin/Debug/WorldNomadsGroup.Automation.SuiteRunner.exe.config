<?xml version="1.0"?>
<configuration>
	<appSettings>
   
    
	  <!-- <add key="TestSuite" value="SuiteDetails.xlsx"/> -->
    <add key="TestSuite" value="SuiteDetails.xml"/>
    <!-- <add key="TestData" value="TestData.xlsx"/>-->
    <add key="TestData" value="TestData.xml"/>
    <add key="Server" value= "http://pp.worldnomads.com"/>
    
    <add key="ReportsPath" value= "C:\Report"/>
    
    
